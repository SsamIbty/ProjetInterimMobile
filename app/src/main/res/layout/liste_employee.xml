<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_height="wrap_content"
        android:layout_width="match_parent"

        android:orientation="horizontal"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:id="@+id/linearColumndeveloppeurweb"
            android:layout_width="151dp"
            android:layout_height="120dp"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_weight="1.0"
            android:background="@color/macolor"
            android:gravity="start"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/eveloppeurweb2"
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:gravity="start"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="86dp"
                        android:layout_height="match_parent"
                        android:gravity="start"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="80dp"
                            android:layout_height="108dp"
                            android:src="@drawable/emploer" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="263dp"
                        android:layout_height="115dp"
                        android:gravity="start"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtagencyname"
                            style="@style/txtInterbold15"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="NAME Lastname"
                            android:textSize="14sp" />

                        <LinearLayout
                            android:id="@+id/linearRoweurone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:gravity="start"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageLocalisateurOne"
                                android:layout_width="@dimen/_12pxh"
                                android:layout_height="@dimen/_15pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_localisateur1" />

                            <TextView
                                android:id="@+id/txtadresse"
                                style="@style/txtInterregular12"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_3pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="adresse"
                                android:textSize="10sp" />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="263dp"
                            android:layout_height="20dp"
                            android:gravity="start"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/imageRevenusOne"
                            android:layout_width="@dimen/_17pxh"
                            android:layout_height="@dimen/_17pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/baseline_link_24" />


                        <TextView
                            android:id="@+id/txtllink"
                            style="@style/txtInterregular10"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:textSize="20px"
                            android:text="Link" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/txttext"
                            style="@style/txtInterregular10"
                            android:layout_width="match_parent"
                            android:layout_height="22dp"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="Ces contraintes permettent de positionner les CardViews de manière à ce qu'elles soient centrées verticalement Ces contraintes permettent de
                             positionner les CardViews de manière à ce qu'elles soient centrées verticalement  "
                            android:textSize="20px" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/txttel"
                                style="@style/txtInterregular10"
                                android:layout_width="150dp"
                                android:layout_height="27dp"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:textSize="20px"
                                android:text="+33 10 12 45 78 96" />
                            <Button
                                android:id="@+id/butmessage"
                                style="@style/txtSolidRounded_1"
                                android:layout_width="100dp"
                                android:layout_height="30dp"
                                android:paddingTop="@dimen/_2pxv"
                                android:background="@drawable/rauond_cyrcle"
                                android:text="Consult"
                                android:textColor="#FFFFFF"
                                android:textSize="8sp" />
                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

